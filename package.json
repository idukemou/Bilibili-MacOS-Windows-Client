    "antd": "^4.15.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "next": "^10.1.3",
    "nextron": "^6.0.6",
    "null-loader": "^4.0.1",
