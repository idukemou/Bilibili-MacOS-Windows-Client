\n\n    componentDidMount() {\n        new Promise(resolve => {\n            if(ipcRenderer){\n                ipcRenderer.send('request_country_list');\n                ipcRenderer.on('fetch_country_list', (event, data) => {\n                    resolve(data)\n                })\n            }\n        }).then(res => {\n            console.log(res);\n            store.dispatch(getCountryListAction(res.data));\n\n        })\n    }\n\n    render(){\n        return (\n            <>\n                <Head>\n                    <title>Home Page</title>\n                </Head>\n                <div>\n                    <Input\n                        placeholder={'Next.js'}\n                        style={{width: 250}}\n                        value={this.state.inputValue}\n                        ref={(input) => this.input = input}\n                        onChange={(e) => this.handleChange(e)}\n                    />\n                    <Button\n                        type={'primary'}\n                        onClick={() => this.handleClick()}\n                    >添加</Button>\n                </div>\n                <div>\n                    <List\n                        bordered\n                        dataSource={this.state.list}\n                        renderItem={(item,index) => (<List.Item onClick={() => this.handleRemoveItem(index)}>{item}</List.Item>)}\n                    />\n                </div>\n            </>\n        )\n    }\n};\n\nexport default Home;\n"]},"metadata":{},"sourceType":"module"}