ipcRenderer || false;\nfunction fetchCountryList(){\n    return new Promise((resolve, reject) => {\n        if(ipcRenderer){\n            ipcRenderer.send('request_country_list');\n            ipcRenderer.on('fetch_country_list', (event, data) => {\n                store.dispatch(getCountryListAction(data));\n                resolve(data);\n            });\n        }\n    }).catch(err => {\n        console.log(err.name + \": \" + err.message);\n    })\n}\n\n\nclass Home extends React.Component{\n\n    static async getInitialProps(ctx){\n        return {\n            loaded: true,\n            countryList: await fetchCountryList()\n        }\n    }\n\n    constructor(props){\n        super(props);\n        this.state = store.getState();\n        this.data = [\n            '早8点开晨会，分配今天的开发工作',\n            '早9点和项目经理作开发需求讨论会',\n            '晚5:30对今日代码进行review'\n        ];\n        store.subscribe(() => this.changeStore());\n    }\n\n    changeStore(){\n        this.setState(store.getState());\n    }\n\n    handleChange = ({target: {value}}) => {\n        store.dispatch(changeInputAction(value));\n    }\n\n    handleClick = () => {\n        store.dispatch(addItemAction());\n    }\n\n    handleRemoveItem = (index) => {\n        store.dispatch(removeItemAction(index));\n    }\n\n    componentDidMount() {\n        console.log(this.props.loaded);\n        console.log(this.props.countryList);\n    }\n\n    render(){\n        return (\n            <>\n                <Head>\n                    <title>Home Page</title>\n                </Head>\n                <div>\n                    <Input\n                        placeholder={'Next.js'}\n                        style={{width: 250}}\n                        value={this.state.inputValue}\n                        ref={(input) => this.input = input}\n                        onChange={(e) => this.handleChange(e)}\n                    />\n                    <Button\n                        type={'primary'}\n                   